#: kcm/ui/main.qml:40
#, kde-format
msgid "Update software:"
msgstr "Ĝisdatigi programaron:"

#: kcm/ui/main.qml:41
#, kde-format
msgid "Manually"
msgstr "Mane"

#: kcm/ui/main.qml:51
#, kde-format
msgid "Automatically"
msgstr "Aŭtomate"

#: kcm/ui/main.qml:58
#, kde-kuit-format
msgctxt "@info"
msgid ""
"Software updates will be downloaded automatically when they become "
"available. Updates for applications will be installed immediately, while "
"system updates will be installed the next time the computer is restarted."
msgstr ""
"Programaj ĝisdatigoj estos elŝutitaj aŭtomate kiam ili estos disponeblaj. "
"Ĝisdatigoj por aplikoj estos instalitaj tuj, dum sistemaj ĝisdatigoj estos "
"instalitaj la venontan fojon kiam la komputilo estos rekomencita."

#: kcm/ui/main.qml:69
#, kde-format
msgctxt "@title:group"
msgid "Update frequency:"
msgstr "Ofteco de ĝisdatigo:"

#: kcm/ui/main.qml:69
#, kde-format
msgctxt "@title:group"
msgid "Notification frequency:"
msgstr "Ofteco de sciigo:"

#: kcm/ui/main.qml:72
#, kde-format
msgctxt "@item:inlistbox"
msgid "Daily"
msgstr "Ĉiutage"

#: kcm/ui/main.qml:73
#, kde-format
msgctxt "@item:inlistbox"
msgid "Weekly"
msgstr "Ĉiusemajne"

#: kcm/ui/main.qml:74
#, kde-format
msgctxt "@item:inlistbox"
msgid "Monthly"
msgstr "Monata"

#: kcm/ui/main.qml:75
#, kde-format
msgctxt "@item:inlistbox"
msgid "Never"
msgstr "Neniam"

#: kcm/ui/main.qml:119
#, kde-format
msgid "Apply system updates:"
msgstr "Apliki sistemajn ĝistdatigojn:"

#: kcm/ui/main.qml:126
#, kde-format
msgctxt ""
"@option:radio part of the logical sentence 'Apply system updates after "
"rebooting'"
msgid "After rebooting"
msgstr "Post restartigo"

#: kcm/ui/main.qml:133
#, kde-format
msgctxt ""
"@label The thing being recommended is to use the 'apply updates when "
"rebooting' setting"
msgid "Recommended to maximize system stability"
msgstr "Rekomendita por maksimumigi sisteman stabilon"

#: kcm/ui/main.qml:139
#, kde-format
msgctxt ""
"@option:radio part of the logical sentence 'Apply system updates immediately'"
msgid "Immediately"
msgstr "Tuje"
